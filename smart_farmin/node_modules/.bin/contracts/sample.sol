pragma solidity >= 0.5.0 < 0.7.0;
contract smartfarm{
 
struct payment{

	// State variables
	uint payment_id;
	uint booking_id;
	string types;
	string amount;
	
	string pay_date;
	

}

 
payment []m;



function add_payment(uint payment_id,uint booking_id,string memory types,string memory amount,string memory pay_date) public{
	payment memory m1=payment(payment_id,booking_id,types,amount,pay_date);
	m.push(m1);
}



}




